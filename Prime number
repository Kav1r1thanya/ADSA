import java.util.*;
import java.io.*;
class Main
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int k=0;
    for(int i=2;i<n-1;i++)
    {
    if(n%i==0)
    {
      k++;
      //System.out.println("Given number is not a prime number");
      }
    }
    if(k!=0)
    {
      System.out.println("Given number is not a prime number");
      }
      
      else
      {
         System.out.println("Given number is a prime number");
      }
      
  }
}
